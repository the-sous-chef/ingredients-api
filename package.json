{
  "name": "@thesouschef/ingredients-api",
  "version": "0.0.1",
  "description": "GraphQL API for ingredients",
  "author": "Brandon Webb (webb.c.brandon@gmail.com)",
  "private": true,
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/the-sous-chef/ingredients/issues"
  },
  "homepage": "https://github.com/the-sous-chef/ingredients#readme",
  "engines": {
    "node": ">=22.7.0"
  },
  "engineStrict": true,
  "scripts": {
    "build": "nest build",
    "format": "prettier . --ignore-unknown --write",
    "format:check": "prettier . --ignore-unknown --check",
    "lint": "eslint .",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "prepare": "is-ci || sh ./scripts/prepare.sh",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:cov": "vitest run --coverage",
    "test:debug": "vitest --inspect-brk --inspect --logHeapUsage --threads=false",
    "test:e2e": "vitest run --config ./vitest.config.e2e.ts",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@apollo/server": "^4.11.0",
    "@as-integrations/fastify": "^2.1.1",
    "@nestjs/apollo": "^12.2.0",
    "@nestjs/common": "^10.4.1",
    "@nestjs/core": "^10.4.1",
    "@nestjs/graphql": "^12.2.0",
    "@nestjs/platform-fastify": "^10.4.1",
    "apollo-server-fastify": "^3.13.0",
    "fastify": "^4.28.1",
    "graphql": "^16.9.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^19.4.1",
    "@nestjs/cli": "^10.4.4",
    "@nestjs/schematics": "^10.1.4",
    "@nestjs/testing": "^10.4.1",
    "@swc/core": "^1.7.21",
    "@thesouschef/eslint-config": "^2.0.1",
    "@thesouschef/prettier": "^2.0.1",
    "@types/node": "^22.5.1",
    "@types/supertest": "^6.0.2",
    "commitlint": "^19.4.1",
    "husky": "^9.1.5",
    "is-ci": "^3.0.1",
    "lint-staged": "^15.2.9",
    "pinst": "^3.0.0",
    "prettier": "^3.3.3",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.5.4",
    "unplugin-swc": "^1.5.1"
  },
  "overrides": {
    "fastify": "^4.28.1"
  },
  "type": "module",
  "volta": {
    "node": "22.7.0"
  }
}
